case $1 in
gcc)
cd _tmp
gcc -I. -L. -o _demo *.cpp $3  -lmpalglib -lmpfr -lgmp -lmpn -lm -lstdc++ >> ../log.txt 2>> ../log.txt
if [ $? -ne 0 ] ; then
echo Error while compiling \(see ../log.txt for more info\)
exit 1
fi
cd ..
pushd _tmp > /dev/null
./_demo
popd > /dev/null
;;
*)
echo unknown compiler
exit 1
;;
esac
exit 0
